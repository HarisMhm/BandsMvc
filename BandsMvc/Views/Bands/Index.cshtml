@using BandsMvc.Models;
@model Band[]

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css">
</head>
<body>
    <h3>Alla band</h3>
    <div>
        <ul>
            @foreach (var item in Model)
            {
                string link = $"/details/{item.Id}";
                <li><a href=@link>@item.Name</a></li>
            }
        </ul>
    </div>
</body>
</html>